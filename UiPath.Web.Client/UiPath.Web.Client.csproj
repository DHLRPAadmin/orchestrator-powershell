  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
    <Target Name="DownloadSwagger" 
            BeforeTargets="GenerateUiPathSwaggerClient"
            Inputs="Swagger\UiPath.swagger"
            Outputs="$(UiPathSwaggerIntermediateLocation)UiPath.swagger">
        <Message 
            Text="DownloadFile $(UiPathSwaggerUrl) -&gt; $(UiPathSwaggerIntermediateLocation)UiPath.swagger" />
        <MakeDir 
            Directories="$(UiPathSwaggerIntermediateLocation)" />
        <DownloadFile 
            Address="$(UiPathSwaggerUrl)" 
            FileName="$(UiPathSwaggerIntermediateLocation)UiPath.swagger" />
        <Exec Command="fc $(UiPathSwaggerIntermediateLocation)UiPath.swagger Swagger\UiPath.swagger" IgnoreExitCode="true">
            <Output TaskParameter="ExitCode" PropertyName="ErrorCode"/>
        </Exec>
        <Copy
            SourceFiles="$(UiPathSwaggerIntermediateLocation)\UiPath.swagger"
            DestinationFolder="Swagger\" Condition="$(ErrorCode)!=0"/>
    </Target>
    <Target Name="GenerateUiPathSwaggerClient" BeforeTargets="BeforeBuild;BeforeRebuild"
            Inputs="Swagger\UiPath.swagger"
            Outputs="@(AutoRestGenerated)">
        <Exec 
            Command="autorest --input-file=Swagger\UiPath.swagger --csharp --csharp.namespace=UiPath.Web.Client --add-credentials"
            Outputs="@(AutoRestGenerated)"/>
    </Target>
</Project>